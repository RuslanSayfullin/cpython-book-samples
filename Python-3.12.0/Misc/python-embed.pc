# See: man pkg-config
prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Python
Description: Embed Python into an application
Requires:
Version: 3.12
Libs.private: -lpthread -ldl  -lutil
Libs: -L${libdir} -lpython3.12d
Cflags: -I${includedir}/python3.12d
